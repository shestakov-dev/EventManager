{
  "arrowParens": "avoid",
  "tabWidth": 4,
  "bracketSpacing": true,
  "useTabs": true,
  "singleQuote": false,
  "semi": true,
  "trailingComma": "es5",
  "bracketSameLine": true,
  "jsxSingleQuote": false,
  "quoteProps": "consistent",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ]
}
